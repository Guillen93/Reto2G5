@using Reto2G5.Model;

@if (students == null)
{
    <p><em>Loading...</em></p>
}
else
{
    <div>
        @for (int i = 0; i < NumberOfRows; i++)
        {
            <div class="row">
                @foreach (var student in students.Skip(i * 6).Take(6))
                {
                    <div class="card studentCard mb-4 bg-@(student.name)">
                        <div class="card-body">
                            <h4 class="card-title">@student.name</h4>
                            <img src="data:image/jpeg;base64," class="img-fluid" alt="@student.name" />
                        </div>
                        <div class="moreinfo">
                            <span class="badge border rounded-pill bg-@(student.name)">
                                <a href="/studentDetails/@student.studentDni"> Leer Más</a>
                            </span>
                        </div>
                    </div>
                }
            </div>
        }
    </div>
}

@code {
    [Parameter]
    public IEnumerable<StudentModel> students { get; set; }
    [Parameter]
    public int NumberOfRows { get; set; }
    //el número máximo de filas por página es 5
}